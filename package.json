  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "firebase": "^10.3.1",
    "moment": "^2.29.4",
    "next": "13.4.19",
    "next-redux-wrapper": "^8.1.0",
    "next-seo": "^6.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.46.1",
    "react-redux": "^8.1.2",
    "redux-persist": "^6.0.0",
    "reselect": "^4.1.8"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/node": "20.5.1",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-packagejson": "^2.4.5",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "redux-devtools-extension": "^2.13.9",
    "tailwindcss": "^3.3.3",
    "typescript": "5.1.6"
  },
  "prettier": {
    "importOrder": [
      "^components/(.*)$",
      "^icons/(.*)$",
      "^images/(.*)$",
      "^flux/(.*)$",
      "^types/(.*)$",
      "^hooks/(.*)$",
      "^services/(.*)$",
      "^utils/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "jsxSingleQuote": false,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "printWidth": 80,
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false
  }
}
